{
  "name": "electron-quick-start-typescript",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "scripts": {
    "build": "tsc && npm run webpack",
    "watch": "tsc -w",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "webpack": "webpack",
    "start:watch": "cross-env NODE_ENV=dev nodemon --exec \"npm run build\" -w && \"electron dist/main.js\"",
    "start": "npm run build && electron dist/main.js"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "electron": "^9.4.0",
    "electron-rebuild": "^2.3.5",
    "eslint": "^7.3.1",
    "node-loader": "^1.0.2",
    "ts-loader": "^8.0.16",
    "typescript": "^3.9.5",
    "wallpaper": "^4.4.2",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@azure/msal-node-extensions": "^1.0.0-alpha.4",
    "@microsoft/mgt-components": "^2.1.0-preview.f66db69",
    "@microsoft/mgt-electron-provider": "^2.1.0-preview.f66db69",
    "@microsoft/mgt-element": "^2.1.0-preview.f66db69",
    "cross-env": "^7.0.3",
    "custom-electron-titlebar": "^3.2.6",
    "electron-reload": "^1.5.0",
    "nodemon": "^2.0.7"
  }
}
